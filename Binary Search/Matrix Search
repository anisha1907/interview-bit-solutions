/* Given a matrix of integers A of size N x M and an integer B.

Write an efficient algorithm that searches for integar B in matrix A */

int Solution::searchMatrix(vector<vector<int> > &A, int B) {
    int n=A.size();
int m=A[0].size();
int i=0,j=m-1;
while(i<n&&j>=0)
{
if(A[i][j]==B) return 1;
if(A[i][j]<B) i++;
else j--;
}
return 0;
}
